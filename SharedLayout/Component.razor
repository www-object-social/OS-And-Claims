@inherits LayoutComponentBase
@inject UnitIdentification.Engine UIE
@inject Product.Infomation PI
@Body
@if (PI.Name is not StandardInternal.product.infomation.Name.ForwardOBJECTSOCIAL or StandardInternal.product.infomation.Name.OBJECTSOCIAL) {
    <audio controls autoplay style="visibility:hidden">
        <source src="_content/SharedLayout/startup.mp3" type="audio/mpeg">
    </audio>
}
<UI_Product_Logo.Component/>
<UI_BrowserSupport.Component/>
